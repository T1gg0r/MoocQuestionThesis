<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mooctest"
    android:versionCode="0"
    android:versionName="0.1" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.uni_hannover.hci.activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.uni_hannover.hci.activity.StartTest"
            android:label="@string/title_activity_start_test"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="de.uni_hannover.hci.activity.Video"
            android:label="@string/title_activity_video"
            android:screenOrientation="landscape" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="YouTubeBaseActivity" />
        </activity>
        <activity
            android:name="de.uni_hannover.hci.quiz.Quiz2"
            android:label="@string/title_activity_quiz"
            android:screenOrientation="landscape" >
        </activity>
    </application>

</manifest>
